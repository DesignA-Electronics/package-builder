CFLAGS="-march=armv5t -mthumb-interwork -mthumb -I${STAGING}/include -specs ${STAGING}/lib/musl-gcc.specs"
#CFLAGS="${CFLAGS} -I${STAGING}/include/linux/include"
#KDIR=/home/andre/work/linux-2.6.33.3
#CFLAGS="${CFLAGS} -I${KDIR}/include"
LDFLAGS="-L${STAGING}/lib -march=armv5t"
PACKAGE_ARCH=musl
export PACKAGE_BUILDER_BASE
export REALGCC=${CROSS}gcc
export CC=${PACKAGE_BUILDER_BASE}/bin/musl-gcc

# Musl has it's own libs, so don't use the toolchain ones
SKIP_TOOLCHAIN_LIBS=y

USE_MUSL=y
